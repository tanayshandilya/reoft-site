<% include elements/header %>
<div class="search-section p-t-b-20">
        <div class="container">
            <h1>Team</h1>
        </div>
    </div>
    <!-- Breadcrumbs -->
    <div class="breadcrumbs">
        <div class="container">
            <ol>
                <li><a href="/">Home</a></li>
                <li class="active">Team</li>
            </ol>
        </div>
    </div>
    <div class="content-wrapper">
        <div class="container">
            <div class="row">
                <div class="col-md-8 p-b-40">
                    <h2>We Are Supper Friendly</h2>
                    <p>Appropriately benchmark bricks-and-clicks e-business before parallel communities. Completely
                        synergize exceptional outsourcing with principle-centered platforms.
                </div>
            </div>
            <div class="row">
            <% for(let i = 0; i < members.length; i++) { %>
                <div class="col-md-4">
                    <div class="paper-block paper-shadow text-center">
                        <img class="p-b-20" height="100px" src="<%= members[i].user_image %>" alt=""/>
                        <a href="<%= members[i].user_profile_slug %>">
                            <h4><%= members[i].user_firstname+' '+members[i].user_lastname %></h4>
                        </a>
                        <h6><%= members[i].user_designation %></h6>
                        <p style="line-height: 1.5"><%= members[i].user_description %></p>
                        <ul class="social p-t-b-10">
                            <li class="linkedin"><a href="#"><i class="icon icon-linkedin"></i></a></li>
                            <li class="facebook"><a href="#"><i class="icon icon-facebook"></i></a></li>
                            <li class="gplus"><a href="#"><i class="icon icon-google"></i></a></li>
                        </ul>
                    </div>
                </div>
            <% } %>
            </div>
        </div>
    </div>
<% include elements/footer %>